  "dependencies": {
    "@aws-cdk/aws-codebuild": "^1.100.0",
    "@aws-cdk/aws-codedeploy": "^1.100.0",
    "@aws-cdk/aws-codepipeline": "^1.100.0",
    "@aws-cdk/aws-codepipeline-actions": "^1.100.0",
    "@aws-cdk/aws-lambda": "^1.100.0",
